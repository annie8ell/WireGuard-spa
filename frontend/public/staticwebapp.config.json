{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/unauthorized",
      "rewrite": "/unauthorized.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["invited"]
    },
    {
      "route": "/index.html",
      "allowedRoles": ["invited"]
    },
    {
      "route": "/",
      "allowedRoles": ["invited"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    },
    "403": {
      "redirect": "/unauthorized",
      "statusCode": 302
    }
  }
}
